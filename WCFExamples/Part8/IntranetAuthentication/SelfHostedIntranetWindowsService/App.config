<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="ServiceBehavior"
      name="SampleService.Samples">
        <endpoint address="" binding="netTcpBinding" name="netTcp"
        contract="SampleService.ISamples" />
        <endpoint address="mex" binding="mexHttpBinding" name="mex"
        contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8089/Samples" />
            <add baseAddress="net.tcp://localhost:8099/Samples" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="ServiceBehavior" >
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
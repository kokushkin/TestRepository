<Window x:Class="TestParallelLine.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid Height="5">
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <Line X1="0" X2="1" Stretch="Fill" Stroke="Black" StrokeThickness="1"/>
            <Line Grid.Row ="1" X1="0" X2="1" Stretch="Fill" Stroke="Black" StrokeThickness="1"/>
        </Grid>
    </Grid>
</Window>
